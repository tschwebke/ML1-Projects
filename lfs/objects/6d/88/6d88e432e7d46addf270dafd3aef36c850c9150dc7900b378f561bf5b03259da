    "fig.subplots_adjust(hspace=0.4, wspace=0.4)\n",
    "\n",
    "# vars_to_plot = ['Relationship_Unknown','Perpetrator Age_0','Perpetrator Sex_Male','Perpetrator Sex_Unknown']\n",
